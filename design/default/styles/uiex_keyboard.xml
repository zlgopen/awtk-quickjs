<button>
  <style name="default" font_size="22" bg_color="#FFFFFF" border_color="#00000000" round_radius="4" text_color="#444444">
    <normal/>
    <disable bg_color="#F9F9F9" text_color="#BDBDBD"/>
    <focused border_color="#709918"/>
    <over/>
    <pressed bg_color="#709918" text_color="#FFFFFF" x_offset="1" y_offset="1"/>
  </style>
  <style name="candidates" margin="5" text_color="#000000" font_size="22">
    <normal/>
    <over border_color="#c2c2c2"/>
    <pressed bg_color="#ffffff" border_color="#c2c2c2"/>
  </style>
  <style name="candidates_focusable" margin="5" text_color="#000000" font_size="22">
    <normal/>
    <disable text_color="#00000066"/>
    <focused border_color="#c2c2c2"/>
    <over border_color="#c2c2c2"/>
    <pressed bg_color="#ffffff" border_color="#c2c2c2"/>
  </style>
  <style name="close" icon="uiex_close_n">
    <normal/>
    <over/>
    <pressed icon="uiex_close_p"/>
  </style>
  <style name="highlight" border_color="#00000000" font_size="22" text_color="#000000" round_radius="4" bg_color="#CACACA">
    <normal/>
    <disable bg_color="#F9F9F9" text_color="#D0D0D0"/>
    <focused bg_color="#FFFFFF" border_color="#709918"/>
    <over/>
    <pressed bg_color="#709918"/>
  </style>
  <style name="highlight_backspace"  border_color="#00000000" text_color="#000000" round_radius="4" icon="uiex_keyboard_backspace_n" bg_color="#CACACA">
    <normal/>
    <disable bg_color="#F9F9F9" text_color="#D0D0D0"/>
    <focused bg_color="#FFFFFF" border_color="#709918"/>
    <over/>
    <pressed bg_color="#709918" icon="uiex_keyboard_backspace_p"/>
  </style>
  <style name="en" border_color="#00000000" icon="uiex_keyboard_EN" round_radius="4" text_color="#000000" bg_color="#CACACA">
    <normal/>
    <disable bg_color="#F9F9F9" text_color="#D0D0D0"/>
    <focused bg_color="#FFFFFF" border_color="#709918"/>
    <over/>
    <pressed bg_color="#709918"/>
  </style>
  <style name="zh" border_color="#00000000" icon="uiex_keyboard_CN" round_radius="4" text_color="#000000" bg_color="#CACACA">
    <normal/>
    <disable bg_color="#F9F9F9" text_color="#D0D0D0"/>
    <focused bg_color="#FFFFFF" border_color="#709918"/>
    <over/>
    <pressed bg_color="#709918"/>
  </style>
</button>
<candidates>
  <style name="default" bg_color="#FAFAFA" text_color="#000000">
    <normal/>
  </style>
</candidates>
<column>
  <style name="placeholder">
    <normal bg_color="#FFFFFF"/>
  </style>
</column>
<keyboard>
  <style name="default">
    <normal bg_color="#E1E1E1" border_color="#CBCBCB"/>
  </style>
</keyboard>
<pages>
  <style name="default">
    <normal border_color="#CBCBCB"/>
  </style>
</pages>
